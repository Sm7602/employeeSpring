<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean id="address1" class=" com.spring.employee.entity.Address" scope="prototype">
		<property name="city" value="salimar"></property>
		<property name="state" value="Delhi"></property>
	</bean>
	
	<bean id="empA" class=" com.spring.employee.entity.Employee2">
		<lookup-method name="applyAddress" bean="address1"/>
	
	</bean>

</beans>