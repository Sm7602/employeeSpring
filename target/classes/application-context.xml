<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean name="add" class="com.spring.employee.entity.Address">
    <property name="city" value="noida"/>
     <property name="state" value="up"/>
    </bean>
    
    <bean name="e1" class="com.spring.employee.entity.Employee">
    <property name="id" value="1"/>
     <property name="name" value="abc"/>
      <property name="gender" value="latter"/>
       <property name="salary" value="2000000"/>
        <property name="address" ref="add"/>
        <property name="list">
         <list>
              <value>123</value>
              <value>1234</value>
              <value>13</value>
              <value>12</value>
              <value>23</value>
         </list>
        </property>
        
        <property name="set">
         <set>
               <value>123</value>
               <value>1234</value>
               <value>13</value>
               <value>12</value>
               <value>23</value>
         </set>
        </property>
        
         <property name="map">
         <map>
                <entry key="1" value="ONE" />
				<entry key="2" value="TWO" />
				<entry key="5" value="FIVE" />
				<entry key="3" value="THREE" />
				<entry key="1" value="one" />
         </map>
        </property>
    </bean>
    
    <bean name="e2" class="com.spring.employee.entity.Employee">
    <constructor-arg name="id" value="2"/>
    <constructor-arg name="name" value="def"/>
    <constructor-arg name="gnder" value="ghdhgd"/>
    <constructor-arg name="salary" value="2051002"/>
     <constructor-arg  name="address" ref="add"/>
     
     <constructor-arg>
         <list>
              <value>123</value>
              <value>1234</value>
              <value>13</value>
              <value>12</value>
              <value>23</value>
         </list>
     </constructor-arg>
     
      <constructor-arg>
           <set>
               <value>123</value>
               <value>1234</value>
               <value>13</value>
               <value>12</value>
               <value>23</value>
         </set>
     </constructor-arg>
     
     <constructor-arg>
          <map>
                <entry key="1" value="ONE" />
				<entry key="2" value="TWO" />
				<entry key="5" value="FIVE" />
				<entry key="3" value="THREE" />
				<entry key="1" value="one" />
         </map>
     </constructor-arg>
     
     
   
    </bean>

</beans>